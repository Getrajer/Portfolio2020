@page "/BankApp"

@inherits BankAppBase

<head>
    <link rel="stylesheet" href="../css/bank_app_style.css" />
</head>

<div class="app_container">



    <!--Navigation-->
    <div class="bank_app_nav">
        <div class="nav_links">
            <div class="toggle_button">

                @if (show_btn_nav)
                {
                    <button @onclick="ToggleNav">
                        <i class="fas fa-bars"></i>
                    </button>

                }
                else
                {
                    <button @onclick="ToggleNav">
                        X
                    </button>
                }




            </div>

            <div class="nav_container @nav_toggle">

                <div class="link">
                    <button @onclick="OpenMainPage">Main Page</button>
                </div>


                @if (is_logged_in == false)
                {
                    <div class="link">
                        <button @onclick="OpenLoginPage">Login/Register</button>
                    </div>
                }
                else
                {
                    <div class="link">
                        <button @onclick="OpenUserPage">@loggedin_user.Name</button>
                    </div>

                    <div class="link">
                        <button @onclick="LogOff">Logoff</button>
                    </div>
                }

            </div>

        </div>
    </div>
    <!--End of navigation-->
    <!--Register Page-->
    @if (register_page)
    {
        <section class="register_page">
            <div class="register_page_container">
                <div class="title">
                    <h1>Register</h1>
                </div>

                <div class="form_container">
                    <div class="input_box">
                        <label>Name</label>
                        <input @bind="name" />
                        <p>@name_error</p>
                    </div>
                    <div class="input_box">
                        <label>Lastname</label>
                        <input @bind="lastname" />
                        <p>@lastname_error</p>
                    </div>
                    <div class="input_box">
                        <label>Email</label>
                        <input @bind="email" />
                        <p>@email_error</p>
                    </div>
                    <div class="input_box">
                        <label>Phone number</label>
                        <input @bind="phone_number" />
                        <p>@phone_number_error</p>
                    </div>
                    <div class="input_box">
                        <label>Postcode</label>
                        <input @bind="postcode" />
                        <p>@postcode_error</p>
                    </div>
                    <div class="input_box">
                        <label>Street Address</label>
                        <input @bind="street_address" />
                        <p>@street_address_error</p>
                    </div>
                    <div class="input_box">
                        <label>Street Number</label>
                        <input @bind="street_number" />
                        <p>@street_number_error</p>
                    </div>

                    <div class="input_date">
                        <div class="age_label">
                            <h2>Date of birth</h2>
                        </div>
                        <div>
                            <label>Day</label>
                            <input @bind="day" type="number" min="1" max="31" />
                        </div>
                        <div>
                            <label>Month</label>
                            <input @bind="month" type="number" min="1" max="12" />
                        </div>
                        <div>
                            <label>Year</label>
                            <input @bind="year" type="number" min="1900" max="@DateTime.Now.Year" />
                        </div>
                    </div>
                    <div class="input_box">
                        <label>Password</label>
                        <input type="password" @bind="password" />
                        <p>@password_error</p>
                    </div>
                    <div class="input_box">
                        <label>Confirm Password</label>
                        <input type="password" @bind="password_confirmation" />
                        <p>@password_confirmation_error</p>
                    </div>
                    <div class="input_box">
                        <label>Password Reminder Question</label>
                        <input @bind="password_reminder_question" />
                        <p>@password_reminder_question_error</p>
                    </div>
                    <div class="input_box">
                        <label>Password Reminder Answer</label>
                        <input @bind="password_reminder_answer" />
                        <p>@password_reminder_answer_error</p>
                    </div>
                    <div class="button_box">
                        <button @onclick="CreateUserAccount">Create Account</button>
                    </div>
                </div>
            </div>
        </section>
    }
    <!--End of create account-->
    <!--Login Page-->
    @if (login_page)
    {
        <section class="login">
            <div class="login_container">
                <div class="title_container">
                    <h1>Login to your account</h1>
                </div>
                <div class="input_container">
                    <label>Email</label>
                    <input @bind="email" />
                    <p>@email_error</p>
                </div>
                <div class="input_container">
                    <label>Password</label>
                    <input type="password" @bind="password" />
                    <p>@password_error</p>
                </div>

                <div class="button_container">
                    <button @onclick="ShowPin">Show Pin/Login Details</button>
                    <p class="error">@pin_toggle_error</p>
                </div>

                <div class="pin_title">
                    <label>PIN</label>
                </div>
                <div class="pin_container">
                    <div class="pin">
                        <div class="pin_wrapper">
                            <div class="pin_title_1">
                                <button @onclick="TogglePin1">@pin_1</button>
                            </div>
                            <div class="@class_pin_1">
                                @foreach (var n in pin_list)
                                {
                                    <div>
                                        <button @onclick="@(e => ChangePin1(n))">@n</button>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>

                    <div class="pin">
                        <div class="pin_title_1">
                            <button @onclick="TogglePin2">@pin_2</button>
                        </div>
                        <div class="@class_pin_2">
                            @foreach (var n in pin_list)
                            {
                                <div>
                                    <button @onclick="@(e => ChangePin2(n))">@n</button>
                                </div>
                            }
                        </div>
                    </div>

                    <div class="pin">
                        <div class="pin_title_1">
                            <button @onclick="TogglePin3">@pin_3</button>
                        </div>
                        <div class="@class_pin_3">
                            @foreach (var n in pin_list)
                            {
                                <div>
                                    <button @onclick="@(e => ChangePin3(n))">@n</button>
                                </div>
                            }
                        </div>
                    </div>

                </div>

                <div class="input_container">
                    <p>@error_login</p>
                </div>
                <div class="button_container">
                    <button @onclick="Login">Login</button>
                </div>

                <div class="register_redirect">
                    <h2>No account?</h2>
                    <button @onclick="OpenRegisterPage">Register</button>
                </div>
            </div>
        </section>
    }
    <!--End of login page-->
    <!--Transfer money page-->
    @if (transfer_checking)
    {
        <section class="transfer">
            <div class="transfer_container">
                <div class="transfer_other">
                    <div class="title_container">
                        <h1>Transfer money to other account</h1>
                    </div>
                    <div class="form_container">
                        <div class="input_container">
                            <label>Ammount of money</label>
                            <input type="number" @bind="money" />
                            <p>@money_error</p>
                        </div>
                        <div class="input_container">
                            <label>Account Number</label>
                            <input @bind="account_number" />
                            <p>@account_number_error</p>
                        </div>
                        <div class="input_container">
                            <label>Sort Code</label>
                            <div class="sortcode">
                                <input maxlength="2" @bind="sort1" /> <span>-</span> <input maxlength="2" @bind="sort2" /> <span>-</span> <input maxlength="2" @bind="sort3" />
                            </div>
                            <p>@sort_error</p>
                        </div>
                        <div class="input_container">
                            <label>Reference</label>
                            <input @bind="reference" />
                            <p>@reference_error</p>
                        </div>
                        <div class="input_container">
                            <label>Password</label>
                            <input type="password" @bind="password" />
                            <p>@password_error</p>
                        </div>
                        <div class="input_container">
                            <label>PIN</label>
                            <input @bind="pin" />
                            <p>@pin_error</p>

                        </div>
                        <div class="buttons_container">
                            <button @onclick="ShowPin">Show Pin/Login Details</button>
                        </div>
                        <div class="buttons_container">
                            <button @onclick="TransferToOthers">Transfer</button>
                        </div>
                        <div class="input_container">
                            <p>@balance_error</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }
    <!--End of transfer money page-->
    <!--Transwer money between user accounts-->
    @if (transfer_to_users)
    {
        <section class="transfer_between_user">
            <div class="transfer_between_user_container">
                <div class="title_container">
                    <h1>Transfer money between your accounts</h1>
                </div>

                <div class="form_container">
                    <div class="input_container">
                        <label>Ammount of money</label>
                        <input type="number" @bind="money" />
                        <p>@money_error</p>
                    </div>

                    <div class="input_container">
                        <label>Select account from which you want to transfer</label>
                        <select @bind="account_chocie_1">
                            @foreach (var o in loggedin_user.bankAccounts)
                            {
                                <option value="@o.Name">@o.Name</option>
                            }
                        </select>
                    </div>

                    <div class="input_container">
                        <label>Select to which account you want transwer to</label>
                        <select @bind="account_chocie_2">
                            @foreach (var o in loggedin_user.bankAccounts)
                            {
                                <option value="@o.Name">@o.Name</option>
                            }
                        </select>
                    </div>

                    <div>
                        <p>@account_choice_error</p>
                        <p>@account_error</p>
                    </div>

                    <div class="input_container">
                        <label>PIN</label>
                        <input maxlength="3" @bind="pin" />
                        <p>@pin_error</p>
                    </div>
                    <div class="buttons_container">
                        <button @onclick="TransferToUsersAccount">Transfer</button>
                    </div>
                </div>
            </div>
        </section>
    }
    <!--End of transwermoney between user accounts-->
    <!--Saving account page-->
    @if (saving_account_page)
    {
        <section class="account_page">
            <div class="account_page_container">

                <div class="title_container">
                    <h1>Welcome to your saving account <span class="name">@loggedin_user.Name</span></h1>
                </div>
                <div class="account_view">
                    <div class="account_view_container">
                        <div class="card_scroll">
                            <div class="card_container">
                                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="361" height="195" viewBox="0 0 361 195">
                                    <defs>
                                        <linearGradient id="Gradient_bez_nazwy_12" data-name="Gradient bez nazwy 12" y1="97.5" x2="361" y2="97.5" gradientUnits="userSpaceOnUse">
                                            <stop offset="0" stop-color="#ba59f8" />
                                            <stop offset="1" stop-color="#814bd4" />
                                        </linearGradient>
                                    </defs>
                                    <title>Zasób 18</title>
                                    <g id="Warstwa_2" data-name="Warstwa 2">
                                        <g id="Warstwa_1-2" data-name="Warstwa 1">
                                            <g>
                                                <rect width="361" height="195" rx="8.52" style="fill: url(#Gradient_bez_nazwy_12)" />
                                                <text transform="translate(124.58 70.49)" style="font-size: 19px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro"><tspan style="letter-spacing: -0.02099609375em">A</tspan><tspan x="11.23" y="0">viable Balan</tspan><tspan x="106.45" y="0" style="letter-spacing: -0.005987870065789474em">c</tspan><tspan x="114.85" y="0">e</tspan></text>
                                                <g>
                                                    <text transform="translate(102.23 116.22)" style="font-size: 50px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro">£</text>
                                                    <text transform="translate(132.23 116.22)" style="font-size: 50px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro">@saving_account.Balance</text>
                                                </g>
                                            </g>
                                        </g>
                                    </g>
                                </svg>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="account_information">
                    <div class="account_information_container">
                        <div class="information">
                            <p>Minimum balance required on the account is £1000£ to generate interest.</p>
                            <p>Your interest rate is @saving_account.Interest%.</p>
                            <p>Interest is add to your account every @saving_account.InterestIteration month(s).</p>
                        </div>

                        <div class="information">
                            <p>Simiulate interest for 1 year time:</p>
                            <p><button @onclick="@(e => SimiulateSafeInterestMonthly(12, saving_account, 1.5))">Simiulate</button></p>
                        </div>

                        @if (show_bank_earnings)
                        {
                            <div class="information">
                                <p>Compound interest: @compound_iterest%</p>
                                <br />
                                <p>Banks are having extra earning's from 3rd decimal point of accumulated interest ammount.</p>
                                <p>During this time bank will earn: £@bank_earnings</p>
                            </div>
                        }

                    </div>
                </div>

                <div class="balance_history">
                    <div class="balance_history_container">
                        <div class="table_container">
                            <div class="title_container">
                                <h2>Balance History</h2>
                            </div>
                            <table>
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Reference</th>
                                        <th>Money</th>
                                        <th>Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var n in ten)
                                    {
                                        <tr>
                                            <td>@(n.Id + 1)</td>
                                            <td>@n.Reference</td>
                                            <td>@n.Money£</td>
                                            <td>@n.DateString</td>
                                        </tr>
                                    }


                                </tbody>
                            </table>
                        </div>

                        <div class="scroll_buttons">
                            @if (ten_min_iterator >= 10)
                            {<button @onclick="@(e => ShowPreviousTen(saving_account.MoneyInOut))">Previous 10</button>}
                            @if (ten_max_iterator < saving_account.MoneyInOut.Count)
                            {<button @onclick="@(e => ShowNextTen(saving_account.MoneyInOut))" class="second_button">Next 10</button>}
                        </div>
                    </div>
                </div>

            </div>
        </section>
    }
    <!--End of saving account page-->
    <!--CD account Page-->
    @if (cd_account_page)
    {
        <section class="account_page">
            <div class="account_page_container">

                <div class="title_container">
                    <h1>Welcome to your CD account <span class="name">@loggedin_user.Name</span></h1>
                </div>
                <div class="account_view">
                    <div class="account_view_container">
                        <div class="card_scroll">
                            <div class="card_container">
                                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="361" height="195" viewBox="0 0 361 195">
                                    <defs>
                                        <linearGradient id="Gradient_bez_nazwy_12" data-name="Gradient bez nazwy 12" y1="97.5" x2="361" y2="97.5" gradientUnits="userSpaceOnUse">
                                            <stop offset="0" stop-color="#ba59f8" />
                                            <stop offset="1" stop-color="#814bd4" />
                                        </linearGradient>
                                    </defs>
                                    <title>Zasób 18</title>
                                    <g id="Warstwa_2" data-name="Warstwa 2">
                                        <g id="Warstwa_1-2" data-name="Warstwa 1">
                                            <g>
                                                <rect width="361" height="195" rx="8.52" style="fill: url(#Gradient_bez_nazwy_12)" />
                                                <text transform="translate(124.58 70.49)" style="font-size: 19px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro"><tspan style="letter-spacing: -0.02099609375em">A</tspan><tspan x="11.23" y="0">viable Balan</tspan><tspan x="106.45" y="0" style="letter-spacing: -0.005987870065789474em">c</tspan><tspan x="114.85" y="0">e</tspan></text>
                                                <g>
                                                    <text transform="translate(102.23 116.22)" style="font-size: 50px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro">£</text>
                                                    <text transform="translate(132.23 116.22)" style="font-size: 50px;fill: #fff;font-family: MyriadPro-Regular, Myriad Pro">@cd_account.Balance</text>
                                                </g>
                                            </g>
                                        </g>
                                    </g>
                                </svg>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="account_information">
                    <div class="account_information_container">
                        <div class="information">
                            <p>Minimum balance required on the account is £30000£. You can't widhdraw from the account for 1 year time as in the contract.</p>
                            <p>Your interest rate is up to 9%.</p>
                            <p>Interest is add to your account every 1 month.</p>
                        </div>

                        <div class="information">
                            <p>Simiulate interest for 1 year time:</p>
                            <p><button @onclick="@(e => SimiulateHigherRiskInterestMonthly(12, cd_account, 9, 5, 3, 2, 4))">Simiulate</button></p>
                        </div>

                        @if (show_bank_earnings)
                        {
                            <div class="information">
                                <p>Compound interest: @compound_iterest%</p>
                                <p>@inflation_message</p>
                                <p>Inflation  rate: @compound_inflation</p>
                                <br />
                                <p>Banks are having extra earning's from 3rd decimal point of accumulated interest ammount.</p>
                                <p>During this time bank will earn: £@bank_earnings</p>
                            </div>
                        }

                    </div>
                </div>

                <div class="balance_history">
                    <div class="balance_history_container">
                        <div class="table_container">
                            <div class="title_container">
                                <h2>Balance History</h2>
                            </div>
                            <table>
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Reference</th>
                                        <th>Money</th>
                                        <th>Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var n in ten)
                                    {
                                        <tr>
                                            <td>@(n.Id + 1)</td>
                                            <td>@n.Reference</td>
                                            <td>@n.Money£</td>
                                            <td>@n.DateString</td>
                                        </tr>
                                    }


                                </tbody>
                            </table>
                        </div>

                        <div class="scroll_buttons">
                            @if (ten_min_iterator >= 10)
                            {<button @onclick="@(e => ShowPreviousTen(cd_account.MoneyInOut))">Previous 10</button>}
                            @if (ten_max_iterator < cd_account.MoneyInOut.Count)
                            {<button @onclick="@(e => ShowNextTen(cd_account.MoneyInOut))" class="second_button">Next 10</button>}
                        </div>
                    </div>
                </div>

            </div>
        </section>
    }
    <!--End of CD account Page-->
    <!--Money market account page-->
    @if (mm_account_page)
    {
        <section class="account_page">
            <div class="account_page_container">

                <div class="title_container">
                    <h1>Welcome to your checking saving <span class="name">@loggedin_user.Name</span></h1>
                </div>
                <div class="account_view">
                    <div class="account_view_container">
                        <div class="card_scroll">
