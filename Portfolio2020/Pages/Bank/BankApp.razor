    @if (user_page)
    {
        <section class="user_page">
            <div class="user_page_container">
                <div class="manage_accounts">
                    <div class="title_container">
                        <h1>Manage your accounts</h1>
                    </div>
                    <div class="manage_accounts_container">
                        @foreach (var n in loggedin_user.bankAccounts)
                        {
                            <div class="button_container">
                                <button @onclick="@(e => ChangeAccountView(n.Name))">@n.Name</button>
                            </div>
                        }

                        <div class="button_container">
                            <button @onclick="OpenTransferInsideUsers">Transwer money between accounts</button>
                        </div>
                    </div>
                </div>


                @if (unsigned_services_names.Count > 0)
                {
                    <div class="apply_for_services">
                        <div class="title_container">
                            <h1>Apply for other services</h1>
                        </div>
                        <div class="apply_for_services_container">
                            @foreach (var s in unsigned_services_names)
                            {
                                <div class="button_container">
                                    <button @onclick="@(e => AddNewServicesView(s))">@s</button>
                                </div>
                            }
                        </div>
                    </div>
                }

            </div>
        </section>
    }
